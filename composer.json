  "require-dev": {
    "latte/latte": "^2.5",
    "nette/application": "^3.0",
    "phing/phing": "^3.0a3",
    "php-coveralls/php-coveralls": "^2.1",
    "php-parallel-lint/php-parallel-lint": "^1.0",
    "phpstan/phpstan": "^0.11.16",
    "phpunit/phpunit": "^8.4",
    "wavevision/coding-standard": "^3.1"
  },
  "scripts": {
    "phing": "phing"
  },
  "extra": {
    "phpstan": {}
  }
