{
  "name": "wavevision/dependent-selectbox",
  "description": "Dependent selectbox component for Nette forms.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wavevision s.r.o",
      "email": "info@wavevision.com"
    },
    {
      "name": "Jakub Filla",
      "email": "jakub.filla@wavevision.com"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "nette/forms": "^3.0",
    "wavevision/utils": "^2.1"
  },
  "require-dev": {
    "latte/latte": "^2.5",
    "nette/application": "^3.0",
    "nette/bootstrap": "^3.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-nette": "^0.12.9",
    "phpunit/phpunit": "^9.4",
    "wavevision/coding-standard": "^5.2"
  },
  "extra": {
    "phpstan": {}
  }
}
